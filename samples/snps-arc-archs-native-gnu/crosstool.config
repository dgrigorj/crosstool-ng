CT_CONFIG_VERSION="3"
CT_EXPERIMENTAL=y
CT_DOWNLOAD_WGET_OPTIONS=" --no-check-certificate"
CT_EXTRA_CFLAGS_FOR_HOST="-O1"
# CT_LOG_PROGRESS_BAR is not set
CT_ARCH_ARC=y
CT_ARCH_CPU="hs38_linux"
# CT_DEMULTILIB is not set
CT_TARGET_CFLAGS="-O2 -ffunction-sections -fdata-sections"
# CT_SHOW_CT_VERSION is not set
CT_TOOLCHAIN_PKGVERSION="ARC HS GNU/Linux glibc toolchain - build 939"
CT_TARGET_VENDOR="snps"
CT_TARGET_ALIAS_SED_EXPR="s/${CT_TARGET}-//"
CT_TARGET_ALIAS="arc"
CT_CANADIAN=y
CT_HOST="arc-snps-linux-gnu"
CT_KERNEL_LINUX=y
CT_LINUX_V_5_1=y
# CT_CC_GCC_STATIC_LIBSTDCXX is not set
# CT_CC_GCC_USE_GRAPHITE is not set
# CT_CC_GCC_ENABLE_TARGET_OPTSPACE is not set
CT_CC_LANG_CXX=y
CT_CC_LANG_FORTRAN=y
CT_DEBUG_GDB=y
CT_GDB_V_10_2=y
# CT_GDB_CROSS is not set
CT_GDB_NATIVE=y
