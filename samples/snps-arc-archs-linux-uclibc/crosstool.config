CT_CONFIG_VERSION="3"
CT_EXPERIMENTAL=y
CT_DOWNLOAD_WGET_OPTIONS=" --no-check-certificate"
# CT_LOG_PROGRESS_BAR is not set
CT_ARCH_ARC=y
CT_ARCH_CPU="archs"
CT_TARGET_CFLAGS="-O2 -ffunction-sections -fdata-sections"
# CT_SHOW_CT_VERSION is not set
CT_TOOLCHAIN_PKGVERSION="ARCv2 ISA Linux uClibc toolchain - build 836"
CT_TARGET_VENDOR="snps"
CT_TARGET_ALIAS_SED_EXPR="s/${CT_TARGET_VENDOR}-//"
CT_TARGET_ALIAS="arc-linux"
CT_KERNEL_LINUX=y
CT_LINUX_V_5_1=y
CT_LIBC_UCLIBC=y
CT_UCLIBC_NG_SRC_DEVEL=y
CT_UCLIBC_NG_DEVEL_URL="https://github.com/wbx-github/uclibc-ng.git"
CT_UCLIBC_NG_DEVEL_BRANCH="v1.0.34"
CT_LIBC_UCLIBC_LOCALES=y
CT_CC_GCC_EXTRA_CONFIG_ARRAY="--with-gnu-as --with-gnu-ld"
# CT_CC_GCC_USE_GRAPHITE is not set
# CT_CC_GCC_ENABLE_TARGET_OPTSPACE is not set
CT_CC_LANG_CXX=y
CT_CC_LANG_FORTRAN=y
CT_DEBUG_GDB=y
CT_GDB_V_10_2=y
# CT_GDB_CROSS_PYTHON is not set
